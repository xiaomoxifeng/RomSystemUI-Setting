<?xml version="1.0" encoding="utf-8"?>
<dashboard-categories
  xmlns:android="http://schemas.android.com/apk/res/android">
    <dashboard-category android:id="@+id/wireless_section" android:title="@string/wifi_setup_connect">
        <dashboard-tile android:icon="@drawable/tw_ic_setting_grid_wifi" android:id="@+id/wifi_settings" android:title="@string/wifi_settings_title" android:fragment="com.android.settings.wifi.WifiSettings" />
        <dashboard-tile android:icon="@drawable/tw_ic_setting_grid_bluetooth" android:id="@+id/bluetooth_settings" android:title="@string/bluetooth_settings_title" android:fragment="com.android.settings.bluetooth.BluetoothSettings" />

<!--HetComm-->
        <dashboard-tile
                android:id="@+id/hetcomm_settings"
                android:icon="@drawable/ic_settings_hetcomm"
                android:title="@string/hetcom_setting_title">
            <intent android:action="com.android.settings.HETCOMM_SETTINGS" />
        </dashboard-tile>

        <dashboard-tile android:icon="@drawable/tw_ic_setting_grid_flightmode" android:id="@+id/flightmode_settings" android:title="@string/airplane_mode" android:fragment="com.android.settings.TwFlightModeSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_hotknot" android:id="@+id/hotknot_settings" android:title="@string/hotknot_settings_title" android:fragment="com.mediatek.settings.hotknot.HotKnotSettings" />
        <dashboard-tile android:icon="@drawable/tw_ic_setting_grid_tethering" android:id="@+id/hotspot_settings" android:title="@string/tw_mobile_hotspot_and_tethering" android:fragment="com.android.settings.TetherSettings" />
        <dashboard-tile android:icon="@drawable/tw_ic_setting_grid_datausage" android:id="@+id/data_usage_settings" android:title="@string/data_usage_summary_title" android:fragment="com.android.settings.DataUsageSummary" />
        <dashboard-tile android:id="@+id/operator_settings" android:fragment="com.android.settings.WirelessSettings">
            <intent android:action="com.android.settings.OPERATOR_APPLICATION_SETTING" />
        </dashboard-tile>
        <dashboard-tile android:icon="@drawable/tw_ic_setting_grid_more" android:id="@+id/wireless_settings" android:title="@string/tw_radio_controls_more_title" android:fragment="com.android.settings.WirelessSettings" />
    </dashboard-category>
    <dashboard-category android:id="@+id/device_section" android:title="@string/header_category_device">
        <dashboard-tile android:icon="@drawable/ic_settings_home" android:id="@+id/home_settings" android:title="@string/home_settings" android:fragment="com.android.settings.HomeSettings" />
        <dashboard-tile android:icon="@drawable/tw_ic_setting_grid_sound" android:id="@+id/notification_settings" android:title="@string/notification_settings" android:fragment="com.mediatek.audioprofile.AudioProfileSettings" />
        <dashboard-tile android:icon="@drawable/tw_ic_setting_grid_display" android:id="@+id/display_settings" android:title="@string/display_settings" android:fragment="com.android.settings.DisplaySettings" />
        <dashboard-tile android:icon="@drawable/ic_setting_grid_motionsandgesture" android:id="@+id/motion_settings" android:title="@string/motion_title" android:fragment="com.android.settings.MotionSettings" />
        <dashboard-tile android:icon="@drawable/tw_ic_setting_grid_application" android:id="@+id/application_settings" android:title="@string/applications_settings" android:fragment="com.android.settings.applications.ManageApplications" />
        <dashboard-tile android:icon="@drawable/ic_settings_multiuser" android:id="@+id/user_settings" android:title="@string/user_settings_title" android:fragment="com.android.settings.users.UserSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_nfc_payment" android:id="@+id/nfc_payment_settings" android:title="@string/nfc_payment_settings_title" android:fragment="com.android.settings.nfc.PaymentSettings" />
        <dashboard-tile android:id="@+id/manufacturer_settings" android:fragment="com.android.settings.WirelessSettings">
            <intent android:action="com.android.settings.MANUFACTURER_APPLICATION_SETTING" />
        </dashboard-tile>
    </dashboard-category>
    <dashboard-category android:id="@+id/personal_section" android:title="@string/header_category_personal">
        <dashboard-tile android:icon="@drawable/tw_ic_setting_grid_wallpaper" android:id="@+id/wallpaper_settings" android:title="@string/tw_settings_category_wallpaper">
            <intent android:targetPackage="com.sec.android.app.wallpapers" android:targetClass="com.sec.android.app.wallpapers.WallpaperPickerActivity" />
        </dashboard-tile>
        <dashboard-tile android:icon="@drawable/tw_ic_setting_grid_location" android:id="@+id/location_settings" android:title="@string/location_settings_title" android:fragment="com.android.settings.location.LocationSettings" />
        <dashboard-tile android:icon="@drawable/tw_ic_setting_grid_security" android:id="@+id/security_settings" android:title="@string/tw_lockscreen_security_settings_title" android:fragment="com.android.settings.SecuritySettings" />
        <dashboard-tile android:icon="@drawable/tw_ic_setting_grid_privacy_safety" android:id="@+id/privacy_settings" android:title="@string/tw_privacy_and_safety_title" android:fragment="com.android.settings.TwPrivacySafetySettings" />
        <dashboard-tile android:icon="@drawable/tw_ic_setting_grid_accessibility" android:id="@+id/accessibility_settings" android:title="@string/accessibility_settings" android:fragment="com.android.settings.accessibility.AccessibilitySettings" />
        <dashboard-tile android:icon="@drawable/tw_ic_setting_grid_accounts" android:id="@+id/account_settings" android:title="@string/account_settings_title" android:fragment="com.android.settings.accounts.AccountSettings" />
        <dashboard-tile android:icon="@drawable/tw_ic_setting_grid_backupandreset" android:id="@+id/privacy_settings" android:title="@string/privacy_settings" android:fragment="com.android.settings.PrivacySettings" />
    <dashboard-tile android:icon="@drawable/tw_ic_setting_grid_simcard" android:id="@+id/sim_settings" android:title="@string/sim_settings_title" android:fragment="com.android.settings.sim.SimSettings" />        
    </dashboard-category>
    <dashboard-category android:id="@+id/system_section" android:title="@string/header_category_system">
        <dashboard-tile android:icon="@drawable/tw_ic_setting_grid_languageandinput" android:id="@+id/language_settings" android:title="@string/language_settings" android:fragment="com.android.settings.inputmethod.InputMethodAndLanguageSettings" />
        <dashboard-tile android:icon="@drawable/tw_ic_setting_grid_storage" android:id="@+id/storage_settings" android:title="@string/storage_settings" android:fragment="com.android.settings.deviceinfo.Memory" />
        <dashboard-tile android:icon="@drawable/tw_ic_setting_grid_battery" android:id="@+id/battery_settings" android:title="@string/power_usage_summary_title" android:fragment="com.android.settings.fuelgauge.PowerUsageSummary" />
        <dashboard-tile android:icon="@drawable/tw_ic_setting_grid_dateandtime" android:id="@+id/date_time_settings" android:title="@string/date_and_time_settings_title" android:fragment="com.android.settings.DateTimeSettings" />
        <dashboard-tile android:icon="@drawable/ic_settings_schpwronoff" android:id="@+id/power_settings" android:title="@string/schedule_power_on_off_settings_title">
            <intent android:action="com.android.settings.SCHEDULE_POWER_ON_OFF_SETTING" />
        </dashboard-tile>
        <dashboard-tile android:icon="@drawable/ic_settings_print" android:id="@+id/print_settings" android:title="@string/print_settings" android:fragment="com.android.settings.print.PrintSettingsFragment" />
        <dashboard-tile android:icon="@drawable/tw_ic_setting_grid_help" android:id="@+id/help_settings" android:title="@string/tw_settings_category_help" />
        <dashboard-tile android:icon="@drawable/tw_ic_setting_grid_developeroption" android:id="@+id/development_settings" android:title="@string/development_settings_title" android:fragment="com.android.settings.DevelopmentSettings" />
        <dashboard-tile android:icon="@drawable/tw_ic_setting_grid_aboutdevice" android:id="@+id/about_settings" android:title="@string/about_settings" android:fragment="com.android.settings.DeviceInfoSettings" />
    </dashboard-category>
</dashboard-categories>
